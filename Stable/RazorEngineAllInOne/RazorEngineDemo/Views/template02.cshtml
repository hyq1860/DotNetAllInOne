@using RazorEngine
@using RazorEngineDemo.ViewModel
@inherits RazorEngine.Templating.TemplateBase<ReportModel>
<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    @Model.ReportId
    @foreach (var photo in Model.Photos)
    {
        if (photo == "111111")
        {
            <img src="@photo" />
        }
        else
        {
            <h1>@photo</h1>
        }
    }
</body>
</html>